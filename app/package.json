  "scripts": {
    "start": "react-scripts start",
    "build": "sed -i '207s/isEnvProduction/false/' node_modules/react-scripts/config/webpack.config.js && node --max-old-space-size=4000 node_modules/react-scripts/scripts/build.js",
    "test": "react-scripts test  --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "prettier-check": "prettier --check './src/**/*.{ts,tsx,json,md}'",
    "ipfs-deploy": "ipfs-deploy build --pinner pinata --no-open --no-clipboard"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prettier-check",
      "pre-push": "yarn run lint"
    }
  },
  "homepage": "./",
  "devDependencies": {
